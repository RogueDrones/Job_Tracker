      "build/**",
      ".git/**",
      "package-lock.json",
      "yarn.lock",
      "public/assets/**",
      "**/*.min.js",
      "**/*.min.css",
      "uploads/**"
    ],
    "words": [
      "autofit",
      "bcrypt",
      "bcryptjs",
      "browserslist",
      "camelcase",
      "cloudinary",
      "CORS",
