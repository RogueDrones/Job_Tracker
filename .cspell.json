{
    "version": "0.2",
    "language": "en",
    "ignorePaths": [
        "node_modules/**",
        "dist/**",
        "build/**",
        ".git/**",
        "package-lock.json",
        "yarn.lock",
        "public/assets/**",
        "**/*.min.js",
        "**/*.min.css",
        "uploads/**"
    ],
    "words": [
        "appbar",
        "asyncHandler",
        "authn",
        "authz",
        "autofit",
        "bcrypt",
        "bcryptjs",
        "browserslist",
        "camelcase",
